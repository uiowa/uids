{
  "name": "@uiowa/uids",
  "version": "1.6.15",
  "description": "The UIowa Design System is specific to the University of Iowa.",
  "keywords": [
    "uiowa",
    "design"
  ],
  "homepage": "https://uids.brand.uiowa.edu",
  "dependencies": {
    "autoprefixer": "^9.8.0",
    "cssnano": "^4.1.10",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sass-glob": "^1.0.9"
  },
  "devDependencies": {
    "@frctl/consolidate": "^1.0.1",
    "@frctl/fractal": "^1.3.0",
    "@frctl/mandelbrot": "^1.3.0",
    "@frctl/twig": "github:frctl/twig#5bc5ed1",
    "mono-fractal": "^1.1.1",
    "stylelint": "^9.8.0",
    "stylelint-config-standard": "^18.2.0",
    "xo": "^0.23.0"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "prebuild": "gulp",
    "build": "fractal build",
    "watch": "gulp watch",
    "start": "fractal start",
    "test": "xo || stylelint src/**/*.css"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
